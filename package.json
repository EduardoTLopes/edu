{
  "name": "edu",
  "version": "0.0.0",
  "description": "CLI to improve devx",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/EduardoTLopes/edu",
  "bugs": "https://github.com/EduardoTLopes/edu/issues",
  "repository": "EduardoTLopes/edu",
  "license": "MIT",
  "author": "Eduardo Lopes",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": "./bin/run.js",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:dev": "shx rm -rf dist && tsc -b --watch",
    "lint": "eslint . --ext .ts",
    "dev": "./bin/dev.js",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "edu",
    "commands": "./dist/commands",
    "dirname": "edu",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@inquirer/checkbox": "^2.3.0",
    "@inquirer/prompts": "^5.0.1",
    "@langchain/community": "^0.0.51",
    "@langchain/core": "^0.1.58",
    "@langchain/openai": "^0.0.28",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "add": "^2.0.6",
    "cheerio": "^1.0.0-rc.12",
    "hnswlib-node": "^3.0.0",
    "langchain": "^0.1.34",
    "ml-distance": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "cheminfo-types": "^1.7.3",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.7.6",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
